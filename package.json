{
  "name": "content-web-extractor",
  "version": "1.0.0",
  "description": "MCP server for extracting content from web pages",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mcp",
    "content-extraction",
    "web-scraping",
    "markdown"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@types/turndown": "^5.0.5",
    "cheerio": "^1.0.0",
    "turndown": "^7.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "rimraf": "^5.0.10",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
